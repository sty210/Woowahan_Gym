<h1>New gymuser</h1>

<%= form_for :gymuser, url: gymusers_path do |f| %>
  <p>
    <%= f.label :gym_no %>
    <%= f.text_field :gym_no, :placeholder=>"input gym_no" %>
  </p>

  <p>
    <%= f.label :user_no %>
    <%= f.text_field :user_no %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', gymusers_path %>
