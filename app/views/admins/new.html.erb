<h1>New admin</h1>

<%= form_for :admin, url: admins_path do |f| %>
  <p>
    <%= f.label :adm_id %>
    <%= f.text_field :adm_id, :placeholder=>"input adm_id" %>
  </p>

  <p>
    <%= f.label :adm_pw %>
    <%= f.text_field :adm_pw %>
  </p>

  <p>
    <%= f.label :adm_nm %>
    <%= f.text_field :adm_nm %>
  </p>

  <p>
    <%= f.label :adm_tel %>
    <%= f.text_field :adm_tel %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', admins_path %>
